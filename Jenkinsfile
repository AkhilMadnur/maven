pipeline {
    agent any
    stages {
        stage('Welcome Step') {
            steps { 
                echo 'Welcome to all world'
            }
        }
         stage('step 1') {
            steps { 
                echo 'Welcome to all world'
            }
         } 
         stage('step 2') {
            steps { 
                echo 'Welcome to everyone world'
            }
         }
         stage('final') {
            steps { 
                echo 'Welcome to 2nd world'
            }
         }
    }
}
